{
  "name": "website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "pnpm sanity:typegen",
    "dev": "SENTRY_SUPPRESS_TURBOPACK_WARNING=1 next dev --turbopack",
    "prebuild": "pnpm sanity:typegen",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pull": "vercel env pull .env.local",
    "sanity:typegen": "pnpx sanity@latest schema extract --path=src/sanity/schema.json && pnpx sanity@latest typegen generate --config-path=src/sanity/sanity-typegen.json"
  },
  "dependencies": {
    "@sanity/asset-utils": "^2.2.0",
    "@sanity/client": "^6.22.5",
    "@sanity/icons": "^3.4.0",
    "@sanity/image-url": "^1.1.0",
    "@sanity/vision": "^3.64.1",
    "@sentry/nextjs": "^8",
    "@vercel/analytics": "^1.4.1",
    "@vercel/flags": "^2.6.3",
    "@vercel/speed-insights": "^1.1.0",
    "@vercel/toolbar": "^0.1.27",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "import-in-the-middle": "^1.11.2",
    "next": "15.0.4",
    "next-sanity": "^9.8.16",
    "react": "19.0.0-rc.1",
    "react-cookie": "^7.2.2",
    "react-dom": "19.0.0-rc.1",
    "react-icons": "^5.3.0",
    "require-in-the-middle": "^7.4.0",
    "sanity": "^3.65.0",
    "statsig-node": "^6.0.1",
    "styled-components": "^6.1.13"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "eslint": "^9.0.0",
    "eslint-config-next": "15.0.4",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  },
  "packageManager": "pnpm@9.14.4",
  "pnpm": {
    "pnpm": {
      "peerDependencyRules": {
        "allowAny": [
          "react",
          "react-dom"
        ]
      }
    }
  }
}
